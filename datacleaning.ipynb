    "    #Separates data within columns for categories/genres/tags to be processed\n",
    "\n",
    "\n",
    "#Lists containing categories/genres/tags to be used\n",
    "categories_keep = [\n",
    "    \"co-op\", \"cross-platform multiplayer\", \"full controller support\",\n",
